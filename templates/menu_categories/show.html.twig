{% extends 'base.html.twig' %}

{% block title %}MenuCategories{% endblock %}

{% block body %}
    <h1>MenuCategories</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ menu_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ menu_category.name }}</td>
            </tr>
            <tr>
                <th>MenuCategoryOrder</th>
                <td>{{ menu_category.menuCategoryOrder }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ menu_category.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_menu_categories_index') }}">back to list</a>

    <a href="{{ path('app_menu_categories_edit', {'id': menu_category.id}) }}">edit</a>

    {{ include('menu_categories/_delete_form.html.twig') }}
{% endblock %}
